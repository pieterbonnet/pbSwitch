		Sub ValueChanged()
		  SwitchW10.Enabled = Me.Value
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events SwitchW10
	#tag Event
		Sub Opening()
		  Me.OnBackcolor = Color.HighlightColor
		  Me.BorderColor = Color.DarkBevelColor
		  Me.BorderVisible = True
		  
		  
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub ValueChanged(NewValue as boolean, ByCode as Boolean = True)
		  #Pragma Unused ByCode
		  chkSynchro.Value = NewValue
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events switchChoice
	#tag Event
		Sub ValueChanged(NewValue as boolean, ByCode as Boolean = True)
		  #Pragma Unused NewValue
		  #Pragma Unused ByCode
		  
		  If Me.OnLeft Then
		    MessageBox Me.RightText// OffText synonym
		  Else // if me.OnRight
		    MessageBox Me.LeftText // onText synonym
		  End
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events chkSynchro
	#tag Event
		Sub ValueChanged()
		  SwitchW10.Value = me.Value
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events SwitchADdemo
	#tag Event
		Sub AfterDrawingSwitch(g as Graphics, areas() as Rect)
		  #Pragma Unused areas
		  
		  If Me.Value Then
		    g.DrawingColor = Me.OnBackcolor
		    g.Bold = True
		    g.DrawText "✓", g.Width - 18, 16
		  Else
		    g.DrawingColor = Me.OffBackcolor
		    g.Bold = True
		    g.FontSize = 20
		    g.DrawText "×", 7, 17
		  End
		End Sub
	#tag EndEvent
#tag EndEvents
