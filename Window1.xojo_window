	#tag Event
		Sub UpPressed()
		  SwitchPG.BallShadowIntensity = SwitchPG.BallShadowIntensity + 5
		  LabelShadow.Text = "Ball Shadow Intensity = " + SwitchPG.BallShadowIntensity.ToString
		End Sub
	#tag EndEvent
