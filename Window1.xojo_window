	#tag Event
		Sub AfterDrawingSwitch(g as Graphics, areas() as Rect, Pressed as Boolean, Hover as Boolean)
		  #Pragma Unused areas
		  
		  If Me.Value Then
		    g.DrawingColor = Me.OnBackcolor
		    g.Bold = True
		    g.DrawText "", g.Width - 17, 16
		  Else
		    g.DrawingColor = Me.OffBackcolor
		    g.Bold = True
		    g.FontSize = 20
		    g.DrawText "", 5, 18
		  End
		End Sub
